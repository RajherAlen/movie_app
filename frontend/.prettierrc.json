{
  "printWidth": 80,
  "tabWidth": 4,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "^react(.*)$",
    "components/(.*)$",
    "features/(.*)$",
    "layouts/(.*)$",
    "hooks/(.*)$",
    "app/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}